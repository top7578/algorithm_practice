#union-find
#1. 부모 노드와 그 전 노드를 union

def find_parent(x):
  if parent[x] != x:
    parent[x] = find_parent(parent[x])
  return parent[x]

def union_parent(a, b):
  a = find_parent(a)
  b = find_parent(b)

  parent[a] = b

g = int(input())
p = int(input())

parent = [0] * (g+1)
for i in range(1, g+1):
  parent[i] = i

cnt = 0
for _ in range(p):
  x = find_parent(int(input()))   #1
  if find_parent(x) == 0:
    break
  else:
    union_parent(x, x-1)          #1
    cnt += 1

print(cnt)
